<div class="header-bg <%- theme.is_article_img?'bg-content-img':''  %>">
    <div class="bg-content">
        <ul class="tag">
            <% if(page.tags && page.tags.data.length) { %>
            <% page.tags.data.forEach((item, index) => { %>
            <li><a href="/tags/<%- item.name %>"><%= item.name %></a></li>
            <% }) %>
            <% } %>
        </ul>
        <h1><%- page.title %></h1>
    </div>
</div>
